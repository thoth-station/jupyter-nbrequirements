{
  "name": "jupyter-nbrequirements",
  "version": "0.0.0",
  "description": "Jupyter magic extension for managing notebook requirements",
  "author": "Marek Cermak <macermak@redhat.com>",
  "homepage": "https://github.com/CermakM/jupyter-nbrequirements#readme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CermakM/jupyter-nbrequirements/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CermakM/jupyter-nbrequirements.git"
  },
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --fix",
    "build": "npm run lint && webpack --config webpack.dev.js",
    "build-prod": "npm run lint && webpack -p --config webpack.prod.js",
    "watch": "webpack -d --watch",
    "build-ts": "rm -rf dist/ && tsc",
    "watch-ts": "tsc --watch",
    "test": "eslint \"**/*.ts\" --quiet"
  },
  "keywords": [
    "dependencies",
    "ipython",
    "jupyter",
    "notebook",
    "requirements",
    "thoth"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "buefy": "^0.8.9",
    "css-loader": "^3.2.0",
    "js-logger": "^1.6.0",
    "lodash": "^4.17.13",
    "style-loader": "^1.0.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-loader": "^15.7.2",
    "vue-property-decorator": "^8.3.0",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.7.3",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.5",
    "webpack-merge": "^4.2.2"
  },
  "types": "types/index.d.ts",
  "_moduleAliases": {
    "@": "dist/src"
  }
}
