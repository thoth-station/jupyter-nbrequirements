{
  "name": "jupyter-nbrequirements",
  "version": "0.0.0",
  "description": "Jupyter magic extension for managing notebook requirements",
  "author": "Marek Cermak <macermak@redhat.com>",
  "homepage": "https://github.com/CermakM/jupyter-nbrequirements#readme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CermakM/jupyter-nbrequirements/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CermakM/jupyter-nbrequirements.git"
  },
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --fix",
    "build": "npm run lint && webpack -d",
    "build-prod": "npm run lint && webpack -p --config webpack.config.prod.js",
    "watch": "webpack -d --watch",
    "build-ts": "rm -rf dist/ && tsc",
    "watch-ts": "tsc --watch",
    "test": "eslint \"**/*.ts\" --quiet"
  },
  "keywords": [
    "dependencies",
    "ipython",
    "jupyter",
    "notebook",
    "requirements",
    "thoth"
  ],
  "dependencies": {
    "js-logger": "^1.6.0",
    "lodash": "^4.17.13"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.0.10",
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.5"
  },
  "types": "types/index.d.ts",
  "_moduleAliases": {
    "@": "dist"
  }
}